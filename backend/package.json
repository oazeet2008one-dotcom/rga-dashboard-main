{
  "name": "rga-dashboard-backend",
  "version": "1.0.0",
  "description": "RGA Marketing Dashboard Backend API",
  "author": "RGA Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nest build",
    "start": "nest start",
    "start:dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nest start --watch",
    "start:dev:prod": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nest build && cross-env NODE_OPTIONS=--max-old-space-size=4096 node dist/main",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "cross-env NODE_OPTIONS=--max-old-space-size=4096 jest --testRegex='.e2e.spec.ts$' --runInBand --forceExit"
  },
